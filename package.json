{
    "name":         "cv"
  , "version":      "0.2.6"
  , "title":        "Modern CV, Resume and Portfolio website"
  , "description":  "Best-in-Class website template for modern CV, Resume and Portfolio. Easy customizable All-in-One-Page static website with simple and smart builder."
  , "homepage":     "https://github.com/tbaltrushaitis/cv"
  , "bugs":         "https://github.com/tbaltrushaitis/cv/issues"
  , "license":      "MIT"
  , "author":       "Baltrushaitis Tomas <tbaltrushaitis@gmail.com>"
  , "private":      false
  , "repository":   {
        "type": "git"
      , "url":  "git+https://github.com/tbaltrushaitis/cv.git"
    }
  , "website":        "http://bit.ly/tomascv"
  , "readmeFilename": "README.md"
  , "engines": {
      "node": ">=8.0.0"
    }
  , "os":     ["linux", "win32", "darwin"]
  , "main":   "echo [CALLED MAIN SCRIPT VIA package.json]"
  , "scripts": {
        "preinstall":    "set echo off; figlet-toilet -t -f small -F border 'NPM:PRE-INSTALL'"
      , "install":       "figlet-toilet -t -f small -F border 'NPM:INSTALL'"
      , "postinstall":   "figlet-toilet -t -f small -F border 'NPM:POST-INSTALL'"
      , "preuninstall":  "figlet-toilet -t -f small -F border 'NPM:PRE-UNINSTALL'"
      , "uninstall":     "figlet-toilet -t -f small -F border 'NPM:UNINSTALL'"
      , "postuninstall": "figlet-toilet -t -f small -F border 'NPM:POST-UNINSTALL'"
      , "start":         "figlet-toilet -t -f small -F border 'NPM:START'"
      , "prepublish":    "figlet-toilet -t -f small -F border 'NPM:PRE-PUBLISH'"
      , "pretest":       "set echo off; echo [PRE-TEST] --- "
      , "test":          "figlet-toilet -t -f small -F border 'NPM:TEST'; DEBUG=* gulp test --color"
      , "posttest":      "figlet-toilet -t -f small -F border 'NPM:POST-TEST'"
      , "\n\n####\t\t\t BUILD:": ""
      , "setup":         "echo [SETUP] --- "
      , "prepare":       "figlet-toilet -t -f small -F border 'NPM:PREPARE'"
      , "populate":      "figlet-toilet -t -f small -F border 'NPM:POPULATE' ; gulp populate --color"
      , "prebuild":      "figlet-toilet -t -f small -F border 'NPM:PRE-BUILD' ; gulp populate --color"
      , "build":         "figlet-toilet -t -f small -F border 'NPM:BUILD' ; gulp build --color"
      , "postbuild":     "figlet-toilet -t -f small -F border 'NPM:POST-BUILD'"
      , "rebuild":       "figlet-toilet -t -f small -F border 'NPM:RE-BUILD'"
      , "prepack":       "figlet-toilet -t -f small -F border 'NPM:PRE-PACK'"
      , "deploy":        "figlet-toilet -t -f small -F border 'NPM:DEPLOY'; gulp deploy --color"
      , "dist":          "figlet-toilet -t -f small -F border 'NPM:DIST'; gulp dist --color"
      , "bower":         "figlet-toilet -t -f small -F border 'NPM:BOWER'; gulp bower --color"
      , "\n\n####\t\t\t BATCH COMMANDS:": ""
      , "default":       "echo [DEFAULT]; DEBUG=* gulp default --color"
      , "all":           "echo [ALL]; DEBUG=* gulp --color"
      , "prod":          "echo [PROD]; gulp --env=production --color"
      , "\n\n####\t\t\t TEST:": ""
      , "check":         "figlet-toilet -t -f small -F border 'NPM:CHECK'; DEBUG=* gulp lint --color"
      , "\n\n####\t\t\t INFORMATION:": ""
      , "config":        "figlet-toilet -t -f small -F border 'NPM:SHOW:CONFIG'; gulp show:config --color"
      , "changed":       "figlet-toilet -t -f small -F border 'NPM:CHANGED'; gulp show:src --color"
      , "list":          "figlet-toilet -t -f small -F border 'NPM:LIST'; gulp --tasks --depth 1 --color"
      , "usage":         "figlet-toilet -t -f small -F border 'NPM:USAGE'; gulp usage --color"
      , "\n\n####\t\t\t DEVELOPMENT:": ""
      , "dev":           "figlet-toilet -t -f small -F border 'NPM:DEV'; DEBUG=* gulp dev --color"
      , "live":          "figlet-toilet -t -f small -F border 'NPM:LIVE'; livereload ./webroot --port 35729 -d -w 1000"
      , "watch":         "figlet-toilet -t -f small -F border 'NPM:WATCH'; DEBUG=* gulp watch --color"
      , "\n# END": ""
    }
  , "scripts-disabled": {
        "crit":       "echo [CRITICAL]; DEBUG=* gulp critical --color"
      , "preinstall": "[ -f ./NODE_ENV ] || cp -prv config/.NODE_ENV ./NODE_ENV ; [ -f ./.bowerrc ] || cp -prv config/.bowerrc ./ ;"
    }
  , "directories": {
      "test": "test"
    }
  , "dependencies": {
    }
  , "dependencies-disabled": {
        "figlet":     "^1.4.0"
      , "figlet-cli": "^0.1.1"
    }
  , "devDependencies": {
        "bower":                "^1.8.8"
      , "dateformat":           "^3.0.3"
      , "del":                  "^5.1.0"
      , "dotenv":               "^8.2.0"
      , "gulp":                 "^3.9.1"
      , "gulp-changed":         "^3.2.0"
      , "gulp-clean-css":       "^3.10.0"
      , "gulp-concat-css":      "^3.1.0"
      , "gulp-directory-sync":  "^1.2.4"
      , "gulp-ext-replace":     "^0.3.0"
      , "gulp-filter":          "^6.0.0"
      , "gulp-headerfooter":    "^1.0.3"
      , "gulp-htmlmin":         "^5.0.1"
      , "gulp-if":              "^3.0.0"
      , "gulp-imagemin":        "^7.1.0"
      , "gulp-jimp":            "^1.2.9"
      , "gulp-jscs":            "^4.1.0"
      , "gulp-jshint":          "^2.1.0"
      , "gulp-livereload":      "^4.0.2"
      , "gulp-replace":         "^1.0.0"
      , "gulp-require-tasks":   "^1.3.0"
      , "gulp-sequence":        "^1.0.0"
      , "gulp-terser":          "^1.1.5"
      , "gulp-token-replace":   "^1.1.2"
      , "gulp-watch":           "^5.0.1"
      , "imagemin-giflossy":    "^5.1.10"
      , "imagemin-mozjpeg":     "^9.0.0"
      , "imagemin-pngquant":    "^9.0.0"
      , "imagemin-webp":        "^6.0.0"
      , "jimp":                 "^0.13.0"
      , "jshint":               "^2.10.1"
      , "jshint-stylish":       "^2.2.1"
      , "js-yaml":              ">=3.13.1"
      , "livereload":           "^0.9.1"
      , "lodash":               "^4.17.14"
      , "lodash.template":      "^4.5.0"
      , "main-bower-files":     "^2.13.3"
      , "merge-stream":         "^2.0.0"
      , "minimist":             ">=1.2.3"
      , "read-config":          "^2.0.0"
      , "vinyl-paths":          "^3.0.1"
      , "websocket-extensions": ">=0.1.4"
      , "yargs":                "^15.3.1"
    }
  , "devDependencies-DISABLED": {
        "critical":           "^1.3.4"
      , "gulp-add-src":       "^1.0.0"
      , "gulp-concat":        "^2.6.0"
      , "gulp-exec":          "^3.0.2"
      , "gulp-file-include":  "^2.0.1"
      , "gulp-newer":         "^1.4.0"
      , "gulp-rename":        "^1.4.0"
      , "gulp-template":      "^5.0.0"
      , "gulp-util":          "^3.0.8"
      , "jquery":             "^3.3.1"
      , "nconf":              "^0.10.0"
      , "nodemon":            "^1.18.10"
    }
  , "options": {
        "bower": {
          "debugging": false
        , "paths": {
              "bowerDirectory": "bower_modules"
            , "bowerrc":        ".bowerrc"
            , "bowerJson":      "bower.json"
          }
        }
      , "clean": {
            "debug":         true
          , "processImport": true
        }
      , "exec": {
            "continueOnError": false
          , "pipeStdout":      false
        }
      , "file": {
            "encoding": "utf8"
          , "flag":     "r"
        }
      , "minify": {
          "suffix": ".min"
        }
      , "reporting": {
            "err":    true
          , "stderr": true
          , "stdout": true
        }
      , "sync": {
            "printSummary": true
          , "nodelete":     true
        }
      , "uglify": {
          "compress": {
              "sequences":     true
            , "properties":    true
            , "dead_code":     true
            , "drop_debugger": true
            , "unsafe":        true
            , "conditionals":  true
            , "comparisons":   true
            , "evaluate":      true
            , "booleans":      true
            , "loops":         true
            , "unused":        true
            , "hoist_funs":    true
            , "hoist_vars":    false
            , "if_return":     true
            , "join_vars":     true
            , "side_effects":  true
            , "warnings":      true
            , "global_defs":   {}
          }
        , "mangle": false
        , "output": {
              "indent_start":  0
            , "indent_level":  2
            , "quote_keys":    false
            , "ascii_only":    false
            , "inline_script": false
            , "width":         80
            , "max_line_len":  32000
            , "beautify":      false
            , "source_map":    null
            , "bracketize":    false
            , "comments":      false
            , "semicolons":    true
          }
        }
      , "watch": {
            "ignoreInitial": false
          , "verbose":       true
          , "readDelay":     200
        }
      , "livereload": {
            "start": true
          , "port":  35729
          , "quiet": false
        }
      , "iopts": {
            "showHidden": false
          , "depth":      4
          , "colors":     true
        }
      , "readconf": {
            "basedir":        null
          , "replaceEnv":     "%"
          , "replaceLocal":   "@"
          , "override":       true
          , "skipUnresolved": true
        }
      , "readconf-disabled": {}
      , "htmlmin": {
            "collapseWhitespace":          true
          , "collapseInlineTagWhitespace": true
          , "conservativeCollapse":        true
          , "removeComments":              true
        }
      , "terser": {
          "keep_fnames": true
        , "mangle":      true
      }
      , "giflossy": {"lossy": 80}
      , "mozjpeg":  {"quality": 50}
      , "pngquant": {"quality": [0.5, 0.5]}
      , "webp":     {"quality": 50}
    }
  , "jshintConfig": {}
  , "keywords": [
        "bio"
      , "cv"
      , "cv-template"
      , "portfolio"
      , "portfolio-website"
      , "portfolio-template"
      , "resume"
      , "skills"
      , "skill-demo"
      , "Technical"
      , "Full-Stack"
    ]
}
